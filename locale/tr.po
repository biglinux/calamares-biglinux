msgstr "<font color='red'><b>Kaldırılamıyor.</b></font><br><br>"

msgid "<font color='red'><b>Não é possível encontrar Gerenciador de Login</b></font><br><br>"
msgstr "<font color='red'><b>Giriş Yöneticisi bulunamıyor</b></font><br><br>"

msgid "VOLTAR"
msgstr "GENE GEL"

msgid "AVANÇAR"
msgstr "İLERLEMEK"

msgid "ERRO!"
msgstr "HATA!"

msgid "SUCESSO!"
msgstr "BAŞARI!"

