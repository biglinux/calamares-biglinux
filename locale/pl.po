msgstr "<font color='red'><b>Nie można usunąć.</b></font><br><br>"

msgid "<font color='red'><b>Não é possível encontrar Gerenciador de Login</b></font><br><br>"
msgstr "<font color='red'><b>Nie można znaleźć Menedżera logowania</b></font><br><br>"

msgid "VOLTAR"
msgstr "WRACAJ"

msgid "AVANÇAR"
msgstr "ZALICZKA"

