msgstr "<font color='red'><b>Nem lehet telepíteni.</b></font><br><br> "

msgid "<font color='red'><b>Não é possível remover.</b></font><br><br>"
msgstr "<font color='red'><b>Nem lehet eltávolítani.</b></font><br><br>"

msgid "<font color='red'><b>Não é possível encontrar Gerenciador de Login</b></font><br><br>"
msgstr "<font color='red'><b>Nem találom a Login Managert</b></font><br><br>"

